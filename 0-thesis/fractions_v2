SELECT
    A.TrdExctnDt,
    A.CusipId,
    ABS(Pt - P) - 
FROM (    
    SELECT
        TrdExctnDt,
        CusipId,
        CustomerBuys / (CustomerBuys + CustomersSells) AS Pt
    FROM (
        SELECT
            TrdExctnDt,
            CusipId,
            SUM(CASE WHEN RptSideCd = 'S' THEN 1 ELSE 0 END) AS CustomerBuys,
            SUM(CASE WHEN RptSideCd = 'B' THEN 1 ELSE 0 END) AS CustomersSells
        FROM
            Trace_filteredWithRatings
        WHERE
            CntraMpId = 'C'
        GROUP BY
            TrdExctnDt,
            CusipId
    ) A
) A
INNER JOIN (
    SELECT
        TrdExctnDt,
        SUM(CustomerBuys) / (SUM(CustomerBuys) + SUM(CustomersSells)) AS P
    FROM (
        SELECT
            TrdExctnDt,
            CusipId,
            SUM(CASE WHEN RptSideCd = 'S' THEN 1 ELSE 0 END) AS CustomerBuys,
            SUM(CASE WHEN RptSideCd = 'B' THEN 1 ELSE 0 END) AS CustomersSells
        FROM
            Trace_filteredWithRatings
        WHERE
            CntraMpId = 'C'
        GROUP BY
            TrdExctnDt,
            IssuerId
    ) A
    GROUP BY
        TrdExctnDt
) B ON A.TrdExctnDt = B.TrdExctnDt
